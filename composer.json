{
    "name": "ezsystems/ezplatform-standard-design",
    "description": "eZ Platform Standard Design Bundle",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0-only",
    "require": {
        "symfony/http-kernel": "^5.0",
        "ezsystems/ezplatform-design-engine": "^3.0",
        "ezsystems/ezplatform-kernel": "^1.0"
    },
    "require-dev": {
        "ezsystems/doctrine-dbal-schema": "^1.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^8.1",
        "ezsystems/ezplatform-code-style": "^0.1.0",
        "friendsofphp/php-cs-fixer": "^2.16.1"
    },
    "autoload": {
        "psr-4": {
            "EzSystems\\EzPlatformStandardDesignBundle\\": "src/bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EzSystems\\Tests\\EzPlatformStandardDesignBundle\\": "tests/bundle"
        }
    },
    "scripts": {
        "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
    },
    "extra": {
        "_ezplatform_branch_for_behat_tests": "master",
        "branch-alias": {
            "dev-tmp_ci_branch": "0.3.x-dev",
            "dev-master": "0.3.x-dev"
        }
    }
}
